config_version=5

[application]

config/name="GDSplit"
config/description="A speedrun timer for Linux and Windows, made in Godot"
run/main_scene="uid://dsdg37o7r8hq"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.5", "Forward Plus")
boot_splash/show_image=false
boot_splash/fullsize=false
config/icon="res://icon.svg"

[autoload]

Globals="*res://scripts/singletons/globals.gd"
SplitMetadata="*res://scripts/singletons/split_metadata.gd"
LayoutMetadata="*res://scripts/singletons/layout_metadata.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=300
window/size/viewport_height=400
window/size/borderless=true
window/size/always_on_top=true
window/size/extend_to_title=true
window/size/sharp_corners=true
window/size/minimize_disabled=true
window/size/maximize_disabled=true
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="LinuxSplit"

[editor]

naming/script_name_casing=2

[global_group]

real_timers="Group of all timers checking \"real time\""

